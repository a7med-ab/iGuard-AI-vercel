{
  "version": 2,
  "builds": [
    {
      "src": "api/search.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/search",
      "dest": "api/search.py",
      "methods": ["POST"]
    },
    {
      "src": "/",
      "dest": "api/search.py"
    }
  ],
  "functions": {
    "api/search.py": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}
